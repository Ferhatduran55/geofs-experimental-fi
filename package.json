{
  "name": "@ferhatduran55/geofs-experimental-fi",
  "description": "Experimental flight interface for GeoFS Pilots.",
  "version": "0.7.13",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/types/index.d.ts",
      "@": "./src"
    }
  },
  "types": "src/types/index.d.ts",
  "keywords": [
    "geo-fs",
    "geofs",
    "enhanced",
    "enhancement",
    "enhancer",
    "pilot",
    "assistant",
    "helper",
    "flight",
    "interface",
    "experimental"
  ],
  "license": "MIT",
  "scripts": {
    "test": "vitest",
    "dev": "tsc && vite build --minify=false && userscript-builder --mode dev",
    "build": "tsc && vite build && userscript-builder --mode dev",
    "release:patch": "tsc && vite build && userscript-builder --mode bugfix",
    "release:minor": "tsc && vite build && userscript-builder --mode minor",
    "release:major": "tsc && vite build && userscript-builder --mode major"
  },
  "userscript": {
    "entry": "./dist/index.js",
    "dev": "./dist",
    "release": "./release",
    "fileName": "index",
    "meta": {
      "name": "Experimental Flight Interface",
      "description": "Improve your plane with the interface that offers experimental features.",
      "namespace": "https://github.com/Ferhatduran55/geofs-experimental-fi",
      "homepage": "https://greasyfork.org/scripts/488227",
      "author": "Ferhatduran55",
      "match": "https://www.geo-fs.com/geofs.php?v=3.82",
      "supportURL": "https://greasyfork.org/scripts/488227/feedback",
      "downloadURL": "https://update.greasyfork.org/scripts/488227.user.js",
      "updateURL": "https://update.greasyfork.org/scripts/488227.meta.js",
      "grant": [
        "GM_addStyle",
        "GM_setValue",
        "GM_getValue",
        "GM_deleteValue",
        "GM_listValues",
        "GM_info",
        "unsafeWindow"
      ],
      "run-at": "document-end"
    }
  },
  "devDependencies": {
    "@types/greasemonkey": "^4.0.7",
    "autoprefixer": "^10.4.19",
    "jsdom": "^24.1.0",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "userscript-builder": "^0.4.0",
    "vite": "^5.3.3",
    "vite-plugin-css-injected-by-js": "^3.5.1",
    "vite-plugin-solid": "^2.10.2",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "solid-js": "^1.8.18",
    "solid-sonner": "^0.2.8"
  }
}
