{
  "name": "@ferhatduran55/geofs-experimental-fi",
  "description": "Experimental flight interface for GeoFS Pilots.",
  "version": "0.7.11",
  "main": "index.user.js",
  "author": "Ferhatduran55",
  "keywords": [
    "geo-fs",
    "geofs",
    "enhanced",
    "experience",
    "enhancement",
    "enhancer",
    "pilot",
